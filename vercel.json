{
  "version": 2,
  "builds": [
    { "src": "src/api/index.ts", "use": "@vercel/node" },
    { "src": "src/dist/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/src/api/index.ts" },
    { "handle": "filesystem" },
    { "src": "/ml-hub", "dest": "/src/dist/ml-hub.html" },
    { "src": "/warehouse-compact", "dest": "/src/dist/warehouse-compact.html" },
    { "src": "/warehouse3d", "dest": "/src/dist/warehouse3d.html" },
    { "src": "/app", "dest": "/src/dist/app.html" },
    { "src": "/login", "dest": "/src/dist/index.html" },
    { "src": "/", "dest": "/src/dist/index.html" },
    { "src": "/(.*)", "dest": "/src/dist/$1" }
  ]
}
