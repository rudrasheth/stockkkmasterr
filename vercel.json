{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/handler.ts" },
    { "src": "/", "dest": "/src/dist/index.html" },
    { "src": "/login", "dest": "/src/dist/index.html" },
    { "src": "/app", "dest": "/src/dist/app.html" },
    { "src": "/app/(.*)", "dest": "/src/dist/app.html" },
    { "src": "/assets/(.*)", "dest": "/src/dist/assets/$1" },
    { "src": "/favicon.ico", "dest": "/src/dist/favicon.ico" },
    { "src": "/robots.txt", "dest": "/src/dist/robots.txt" },
    { "handle": "filesystem" },
    { "src": "/(.*\\.(js|css|map|json|svg|png|jpg|jpeg|webp|ico|txt))$", "dest": "/src/dist/$1" },
    { "src": "/(.*)", "dest": "/src/dist/index.html" }
  ]
}
